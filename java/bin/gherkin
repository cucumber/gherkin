#!/usr/bin/env bash
set -euf -o pipefail
DIR=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )

messages_version=6.0.2
gherkin_version=8.1.0

java \
    -classpath "${DIR}/../target/gherkin-${gherkin_version}.jar:${HOME}/.m2/repository/io/cucumber/messages/${messages_version}/messages-${messages_version}.jar" \
    io.cucumber.gherkin.Main $*
